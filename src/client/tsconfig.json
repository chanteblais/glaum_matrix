{
  "extends": "@tsconfig/node12/tsconfig.json",
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "target": "es2019",
    "jsx": "react",
    "module": "commonjs",
    "sourceMap": true,
    "skipLibCheck": true,
    "useUnknownInCatchVariables": false,
    "noImplicitAny": false,
    "lib": [
      "DOM"
    ]
  }
}
